                android:bottomRightRadius="5dp"
                android:topLeftRadius="5dp"
                android:topRightRadius="5dp" />

        </shape>

    </item>
    <item
        android:drawable="@drawable/ic_baseline_keyboard_arrow_down_24"
        android:gravity="right|center_vertical">
    </item>
</layer-list>
